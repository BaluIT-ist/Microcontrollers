#include "stm32f3xx.h"
//1. Change the program to turn on all LEDs when the button is pressed.
int main(void){

	RCC->AHBENR |=(1UL<<18U)|(1UL<<17U);
	GPIOB->MODER=0X00055000;

	while(1)
	{
		if ((GPIOA->IDR & 0x00000001) == 0x1) {
		    GPIOB->ODR = 0x3C0;
		} else {
		    GPIOB->ODR = 0x000;
		}
	}
	return 0;
}
